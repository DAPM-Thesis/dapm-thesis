{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://dapm.com/schemas/processing.element.reference.schema.json",
  "title": "Processing Element Reference",
  "type": "object",
  "properties": {
    "organizationID":
    {
      "type": "string",
      "minLength": 1,
      "pattern": "^(?!\\s*$)[^\\t\\n\\r]*$"
    },
    "templateID":
    {
      "type": "string",
      "minLength": 1,
      "pattern": "^(?!\\s*$)[^\\t\\n\\r]*$"
    },
    "inputs":
    {
      "type": "array",
      "items": { "$ref": "https://dapm.com/schemas/message.type.schema.json"}
    },
    "output": {
      "anyOf": [
        { "$ref": "https://dapm.com/schemas/message.type.schema.json" },
        { "type": null }
      ]
    },
    "instanceNumber": { "type":  "integer", "minimum": 1},
    "parameterValues": { "type":  "object"}
  },
  "required": ["organizationID", "templateID", "inputs", "output", "instanceNumber"],
  "additionalProperties": false
}
